                                               echo 'inside stage one and step one'
                                                }
                                          steps {
                                                 echo 'inside stage one step two'
                                                 } 
                                        
                       }
                       stage('stage_two') {
                                          steps {
                                               echo 'inside stage two and step 1'
                                                }
                       }

                 }
}




